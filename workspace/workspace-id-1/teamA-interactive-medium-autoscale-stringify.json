"name": "Test policy",
"definition": "{\n  \"cluster_type\": {\n    \"type\": \"fixed\",\n    \"value\": \"all-purpose\"\n  },\n  \"autoscale.min_workers\": {\n    \"type\": \"range\",\n    \"maxValue\": 10,\n    \"defaultValue\": 2\n  },\n  \"dbus_per_hour\": {\n    \"type\": \"range\",\n    \"maxValue\": 60\n  },\n  \"autoscale.max_workers\": {\n    \"type\": \"range\",\n    \"maxValue\": 30,\n    \"defaultValue\": 4\n  },\n  \"autotermination_minutes\": {\n    \"type\": \"range\",\n    \"maxValue\": 120,\n    \"defaultValue\": 60\n  },\n  \"node_type_id\": {\n    \"type\": \"blocklist\",\n    \"values\": [\"z1d.12xlarge\", \"z1d.6xlarge\", \"r5d.16xlarge\", \"r5a.24xlarge\", \"c5.24xlarge\"],\n    \"defaultValue\": \"i3.xlarge\"\n  },\n  \"driver_node_type_id\": {\n    \"type\": \"blocklist\",\n    \"values\": [\"z1d.12xlarge\", \"z1d.6xlarge\", \"r5d.16xlarge\", \"r5a.24xlarge\", \"c5.24xlarge\"],\n    \"defaultValue\": \"i3.xlarge\"\n  },\n  \"spark_version\": {\n    \"type\": \"regex\",\n    \"pattern\": \"10\\\\.[0-9]+\\\\.x-scala.*\"\n  },\n  \"enable_elastic_disk\": {\n    \"type\": \"fixed\",\n    \"value\": true,\n    \"hidden\": true\n  },\n  \"custom_tags.team\": {\n    \"type\": \"fixed\",\n    \"value\": \"teamA\"\n  }\n}"
